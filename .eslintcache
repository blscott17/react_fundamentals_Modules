.then((data) => setResults(data.response.docs))\r\n      .catch((err) => console.log(err));\r\n  };\r\n\r\n  const handleSubmit = (event) => {\r\n    event.preventDefault();\r\n    fetchResults();\r\n  };\r\n\r\n  return (\r\n    <div className='main'>\r\n      <div className='mainDiv'>\r\n        <form onSubmit={(e) => handleSubmit(e)}>\r\n          <span>Enter a single search term (required) : </span>\r\n          <input\r\n            type='text'\r\n            name='search'\r\n            onChange={(e) => setSearch(e.target.value)}\r\n            required\r\n          />\r\n          <br />\r\n          <span>Enter a start date: </span>\r\n          <input\r\n            type='date'\r\n            name='startDate'\r\n            pattern='[0-9]{8}'\r\n            onChange={(e) => setStartDate(e.target.value)}\r\n          />\r\n          <br />\r\n          <span>Enter an end date: </span>\r\n          <input\r\n            type='date'\r\n            name='endDate'\r\n            pattern='[0-9]{8}'\r\n            onChange={(e) => setEndDate(e.target.value)}\r\n          />\r\n          <br />\r\n          <button className='submit'>Submit search</button>\r\n        </form>\r\n        {/* Added if we have results we pass them as a prop to NytResults like below: */}\r\n        {results.length > 0 ? <NytResults results={results} /> : null}\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default NytApp;\r\n","C:\\__BLUEBADGE\\05-Node-Server\\react_fundamentals_Modules\\src\\components\\apps\\nyt-app\\NytResults.js",[],{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","severity":1,"message":"111","line":31,"column":7,"nodeType":"112","endLine":31,"endColumn":68},{"ruleId":"113","severity":1,"message":"114","line":36,"column":7,"nodeType":"112","endLine":41,"endColumn":9},{"ruleId":"115","severity":1,"message":"116","line":9,"column":19,"nodeType":"117","messageId":"118","endLine":9,"endColumn":29},{"ruleId":"115","severi